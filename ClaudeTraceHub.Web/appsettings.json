{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureDevOps": {
    "ApiVersion": "5.0",
    "BranchWorkItemPatterns": [
      "^(?:feature|bug|hotfix|task|requirement|cr|mgr)/?\\s*(\\d+)",
      "(\\d{4,})"
    ]
  }
}
